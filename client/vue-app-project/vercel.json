{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "rewrites": [
    {
      "src": "/api/(.*)",
      "dest": 1,
      "has": [
        {
          "type": "host",
          "value": "api.example.com"
        }
      ]
    }
  ]
}
